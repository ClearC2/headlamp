{
  "name": "headlamp",
  "version": "1.1.0",
  "description": "Express API documenter",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Server,React\" \"npm run server\" \"npm run react\"",
    "react": "cd api-explorer && cross-env NODE_ENV=development webpack-dev-server -d --hot",
    "server": "cross-env nodemon test-api/server.js --watch src --watch test-api --exec babel-node",
    "build": "npm run build-backend && npm run build-frontend",
    "build-backend": "node_modules/.bin/babel -d lib/ src/",
    "build-frontend": "cd api-explorer && cross=env NODE_ENV=production webpack --mode production",
    "babel-node": "babel-node",
    "cm": "git-cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "extends": "c2-semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClearC2/headlamp.git"
  },
  "author": "Clear C2, Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClearC2/headlamp/issues"
  },
  "homepage": "https://github.com/ClearC2/headlamp#readme",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "c2-semantic-release": "ClearC2/c2-semantic-release#^1.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.10",
    "eslint-config-c2-react": "^1.3.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.4",
    "lodash": "^4.17.10",
    "markdown-it": "^8.4.1",
    "moment": "^2.22.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.5",
    "open-iconic": "^1.1.1",
    "path-to-regexp": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.2.0",
    "react-paginate": "^5.2.3",
    "react-router-dom": "^4.2.2",
    "react-spinkit": "^3.0.0",
    "react-syntax-highlighter": "^7.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "url-search-params-polyfill": "^4.0.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "dependencies": {
    "glob": "^7.1.2"
  }
}
